<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:personnel="clr-namespace:EnduranceJudge.Gateways.Desktop.Views.Content.Event.Children.Personnel">

    <DataTemplate DataType="{x:Type personnel:PersonnelViewModel}">
        <StackPanel Style="{StaticResource List-Horizontal}">
            <TextBlock Style="{StaticResource Text}"
                       Text="{Binding RoleName}" />

            <TextBlock Style="{StaticResource Text}"
                       Text="{Binding Name}" />
        </StackPanel>
    </DataTemplate>

</ResourceDictionary>
